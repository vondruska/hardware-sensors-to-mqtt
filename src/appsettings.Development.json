{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Warning",
      "HardwareSensorsToMQTT.*": "Debug"
    }
  },
  "HardwareSensorsToMQTT": {
    "Name": "foobar",
    "SensorsToPublish": [
      {
        "id": "/ram/data/0",
        "interval": "00:00:03"
      },
      {
        "id": "/ram/data/1",
        "interval": "00:00:03"
      },
      {
        "id": "/foo",
        "interval": "00:00:03"
      }
    ],
    "Mqtt": {
      "Host": "broker.hivemq.com"
    }
  }
}